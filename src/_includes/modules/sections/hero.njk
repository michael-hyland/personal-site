{% module "section", "hero" %}
<div class="text-content">
    
    <h1>{{ jumboText | markdownify | safe | replace("<p>", "") }}</h1>

    {% button "mailto:" + link.url, link.text, "primary-btn" %}
</div>

<picture class="hero-image">
    <img src="images/computer.webp" alt="3d vector of retro desktop computer">
</picture>
{% endmodule %}   